# Solana RPC Configuration
SOLANA_RPC_URL=https://solana-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY

# Discord Webhook
DISCORD_WEBHOOK_URL=YOUR_DISCORD_WEBHOOK_URL_HERE

# Paper Trading Configuration
PAPER_MODE=true
STARTING_CAPITAL=20
SLIPPAGE_TOLERANCE=0.03
MIN_PROFIT_PERCENT=0.005
MIN_PROFIT_ABSOLUTE=0.10

# Rate Limiting (minutes)
UPDATE_INTERVAL=15

# Database
DB_PATH=./data/trading.db

# Monitoring Pairs (comma-separated, e.g., SOL/USDC,RAY/USDC)
MONITOR_PAIRS=SOL/USDC,RAY/USDC,BONK/USDC

# DEX Configuration
JUPITER_API=https://price.jup.ag/v6
RAYDIUM_API=https://api-v3.raydium.io
PHOENIX_ENABLED=false
